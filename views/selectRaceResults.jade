extends layout

block content
  h2 Select one of the races below to see results
  table
    thead
      tr
        th Race ID
        th(colspan="10") Titles
    tbody    
      - for (i = 0; i < races.length; ++i)
        tr
          td(style="text-decoration:none;")
            - var link = "/results/" + races[i][0];
            a(href=link)=races[i][0]
          - for (j = 1; j < races[i].length; ++j)
            td=races[i][j]
